|string allbutnewline statmentIdentifier tokenIdentifier|allbutnewline := (#word asParser / #blank asParser/ #punctuation asParser)plus.statmentIdentifier := (allbutnewline, #newline asParser star) trim flatten.tokenIdentifier := (#word asParser plus/ #punctuation asParser plus)flatten trim.string := 'ad adf .. { 01 10 d1 1d ', Character cr asString, 'adsfasdf 4 sdf54 '.tokenIdentifier   matchesSkipIn: string.