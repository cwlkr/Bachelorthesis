|timestamp  day hour minute second|timestamp := DateAndTime current asString.day := ''.hour:= ''.minute := ''.second := ''.(1 to: 10 )do:[ :a|	day:= day , (timestamp at: a) asString].(11 to: 13 )do:[ :a|	hour:= hour , (timestamp at: a) asString].(15 to: 16 )do:[ :a|	minute:= minute , (timestamp at: a) asString].(18 to: 19 )do:[ :a|	second:= second , (timestamp at: a) asString].timestamp :=  day, hour, minute, second.